def fibonacci(n):
    a, b = 0, 1
    while b <= n:
        a, b = b, a + b
    if b == n:     
        return True
    else:
        return False

numero = int(input('Digite um número:'))
if fibonacci(numero):
    print(f'{numero} pertence à sequencia de Fibonacci.')
else:
    print(f'{numero} não pertence a sequência de Fibonacci.')
                       

	
